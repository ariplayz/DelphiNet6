<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="500"
             x:Class="DelphiNet6.Controls.ProgramWidget">
    <StackPanel Orientation="Vertical" Background="{DynamicResource PrimaryBackgroundGradient}">
        <TextBlock Background="{DynamicResource PrimaryForeground}" FontSize="35">Program</TextBlock>
        <Grid ColumnDefinitions="230, 110, 60" RowDefinitions="*" Background="{DynamicResource PrimaryHeaderBackground}">
            
        </Grid>
    </StackPanel>
</UserControl>
